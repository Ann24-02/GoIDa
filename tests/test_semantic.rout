-- Valid program with various constructs
var x: integer is 5
var y: integer is 10

routine calculate(a: integer, b: integer) return integer is
    var result: integer is a + b
    return result
end

routine main() is
    var total: integer is calculate(x, y)
    print total
    
    if total > 0 then
        print "Positive"
    else
        print "Non-positive"
    end
    
    for i in 1..5 loop
        print i
    end
end