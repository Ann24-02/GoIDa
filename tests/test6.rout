-- shows type conversions between real, integer, and boolean, then prints results

type MyInt is integer
type MyReal is real

routine main() is
    var r : MyReal is 3.6
    var i : MyInt
    var b : boolean is false

    -- real -> integer (round to nearest)
    i := r

    -- boolean -> real (false -> 0.0)
    r := b

    -- boolean -> integer (false -> 0)
    i := b

    print i, r
end
