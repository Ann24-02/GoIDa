-- Valid program with various constructs
var x: integer is 5;
var y: integer is 10;

routine calculate(a: integer, b: integer) is
    var result: integer is a + b;
    print "Result:", result;
end;

routine main() is
    calculate(x, y);
    var total: integer is x + y;
    print "Total:", total;
    
    if total > 0 then
        print "Positive";
    else
        print "Non-positive";
    end;
    
    for i in 1..5 loop
        print "Loop iteration:", i;
    end;
end;